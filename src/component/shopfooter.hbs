<div class="row g-1">
    {{#each ftr}}
    <div class="nav col-2">
        {{#each this}}
        <div class="nav nav-item">
            {{#if @last}}
            <span class="ps-3">{{text}}</span>
            {{else}}
            <a class="nav-link" href={{link}}>{{text}}</a>
            {{/if}}
        </div>
        {{/each}}
    </div>
    {{/each}}
</div>